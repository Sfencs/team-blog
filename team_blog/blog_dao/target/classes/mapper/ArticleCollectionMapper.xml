<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.thefifth.dao.IArticleCollection" >


    <insert id="toCollecArticle" parameterType="com.thefifth.bean.ArticleCollection">
        insert into articlecollection(user_id,article_id)
        values (#{user_id},#{article_id})
    </insert>

    <select id="hasCollectArticles" parameterType="int" resultType="com.thefifth.bean.ArticleCollection">
        select * from articlecollection where user_id=#{user_id}
    </select>

</mapper>